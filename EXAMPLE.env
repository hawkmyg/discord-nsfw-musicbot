# Discord bot token (required)
DISCORD_TOKEN=YOUR_BOT_TOKEN_HERE

# Discord bot application/client ID (required for some deploys)
CLIENT_ID=YOUR_BOT_CLIENT_ID_HERE

# Guild/server ID for slash command registration (optional in production)
GUILD_ID=YOUR_GUILD_ID_HERE (SERVER ID)

# Sightengine moderation credentials (if using NSFW/image moderation features)
SIGHTENGINE_USER=your_sightengine_user
SIGHTENGINE_SECRET=your_sightengine_secret



# If these are changed while bot is running use /reload hit enter and give new settings a minute to register!
# Music room voice channel
# Leave blank to allow /play in any voice channel)
MUSIC_ROOM_VOICE_ID=YOUR_MUSIC_ROOM_VOICE_CHANNEL_ID_HERE

# Only allow /play in this request text channel (required for restriction) 
# Leave blank to allow /play in any text channel.
REQUEST_TEXT_CHANNEL_ID=YOUR_REQUEST_TEXT_CHANNEL_ID_HERE